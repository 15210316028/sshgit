<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ssh.zhu.model">
    <class name="goods"  table="goods">
        <id name="id" column="id" type="java.lang.Integer"></id>
        <property name="name" column="name" type="java.lang.String" />
        <property name="yield" column="yield" type="java.lang.Double" />
        <property name="time" column="time" type="java.lang.String" />
        <property name="money" column="money" type="java.lang.Double" />
        <property name="surplus" column="surplus" type="java.lang.Double" />
        <property name="kind" column="kind" type="java.lang.String"></property>
        <property name="url" column="url" type="java.lang.String"></property>
    </class>
</hibernate-mapping>